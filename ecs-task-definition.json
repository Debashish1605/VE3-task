{
    "family": "my-task-def",
    "networkMode": "awsvpc",
    "containerDefinitions": [
      {
        "name": "my-container",
        "image": "180294178670.dkr.ecr.ap-south-1.amazonaws.com/my-web-app",
        "essential": true,
        "memory": 512,
        "cpu": 256,
        "portMappings": [
          {
            "containerPort": 80,
            "hostPort": 80
          }
        ]
      }
    ],
    "requiresCompatibilities": ["FARGATE"],
    "cpu": "256",
    "memory": "512",
    "executionRoleArn": "arn:aws:iam::180294178670:role/ecsTaskExecutionRole",
    "taskRoleArn": "arn:aws:iam::180294178670:role/ecsTaskExecutionRole"
  }
  